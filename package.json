        "core-js": "2.4.1",
        "lite-server": "2.2.2",
        "reflect-metadata": "0.1.9",
        "rxjs": "5.1.1",
        "systemjs": "0.20.7",
        "ts-loader": "2.0.0",
        "typescript": "2.1.6",
        "zone.js": "0.7.7"
